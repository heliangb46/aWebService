<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="message">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="head">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="response">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="esbRespType" type="xs:string"/>
                    <xs:element name="esbRespCode" type="xs:string"/>
                    <xs:element name="esbRespDesc" type="xs:string"/>
                    <xs:element name="transRefGUID" type="xs:string"/>
                    <xs:element name="transNo" type="xs:string"/>
                    <xs:element name="relateRows" type="xs:string"/>
                    <xs:element name="businessRespCode" type="xs:string"/>
                    <xs:element name="businessRespText" type="xs:string"/>
                    <xs:element name="businessRespTime" type="xs:string"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="body">
          <xs:complexType>
            <xs:sequence/>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>

